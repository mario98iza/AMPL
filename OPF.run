#RUN
reset;

# Cargar modelo y datos
model OPF1.mod;
data OPF.dat;

# Configurar solver
option solver knitro;

# Resolver el problema de optimizaci√≥n
solve generacion_slack;

# Verificar y mostrar resultado
display V,th;
